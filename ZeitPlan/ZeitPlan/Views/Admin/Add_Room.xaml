<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="ZeitPlan.Views.Admin.Add_Room">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="txtRoom_No" Placeholder="Room No" Keyboard="Numeric" />
            <Entry x:Name="txtRDepartmentFID" Placeholder="DepartmentFID" Keyboard="Numeric" />
            <Entry x:Name="txtRtblDepartmentFID" Placeholder="TBL_DepartmentFID" Keyboard="Numeric" />
            <Entry x:Name="txtRtblTimeTableFID" Placeholder="TBL_TimeTableFID" Keyboard="Numeric" />
            <Button Text="Add Room" x:Name="btnRoom" Clicked="btnRoom_Clicked"/>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>